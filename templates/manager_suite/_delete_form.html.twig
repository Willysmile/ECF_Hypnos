<form method="post" action="{{ path('app_manager_suite_delete', {'id': suite.id}) }}"
      onsubmit="return confirm('Êtes-vous sûrs de supprimer cette suite?');">

    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ suite.id) }}">
    <button class="btn btn-danger btn-sm m-3 ">Suppression de la suite</button>
</form>
